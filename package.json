{
    "name": "autoclaim-bot",
    "module": "src/index.ts",
    "type": "module",
    "private": true,
    "scripts": {
        "start": "bun run deploy && bun run src/index.ts",
        "dev": "bun --watch run src/index.ts",
        "deploy": "bun run src/deploy-commands.ts",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/node-cron": "^3.0.11",
        "@types/speedtest-net": "^2.1.5",
        "prettier": "^3.8.1"
    },
    "peerDependencies": {
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@cloudflare/speedtest": "^1.7.0",
        "axios": "^1.13.4",
        "discord.js": "^14.25.1",
        "mongoose": "^9.1.5",
        "node-cron": "^4.2.1",
        "speedtest-net": "^2.2.0"
    },
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "printWidth": 120
    }
}
